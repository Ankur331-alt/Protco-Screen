<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 阴影图片，android:left表示阴影图片左边到背景图片左边的距离
    android:top表示阴影图片上边到背景图片上边的距离-->
    <item android:right="@dimen/dpp_32" >
        <shape android:shape="rectangle">
            <size android:width="@dimen/dpp_8"
                android:height="@dimen/dpp_32" />
            <corners android:radius="@dimen/dpp_4" />
            <solid android:color="@color/cen_connect_step_selected_color" />
        </shape>
    </item>
</layer-list>
